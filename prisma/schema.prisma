generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model BuildingProject {
  id Int @id @default(autoincrement())
  title String @default("")
  urban_or_architecture String @default("")
  project_category  String @default("")
  region String @default("")
  plot_area String @default("")
  building_type String @default("")
  basic_limits String @default("")
}